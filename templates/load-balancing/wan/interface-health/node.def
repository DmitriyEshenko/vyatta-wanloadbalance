tag:
type: txt
help: interface name
syntax:expression: exec " \
        if [ -z \"`ip link | egrep -v 'eth[0-9]+[.]' | grep $VAR(@)`\" ]; then \
          echo Invalid interface [$VAR(@)]; \
          exit 1 ; \
        fi ; "
allowed: for dev in /sys/class/net/*; 
         do if [[ -L $dev/device ]]
            then echo -n ${dev##*/} " "
            fi
         done
